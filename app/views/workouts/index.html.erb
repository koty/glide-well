<h1>Workouts#index</h1>
<p>Find me in app/views/workouts/index.html.erb</p>
<ul class="users">
  <%= link_to '新規登録', new_workout_path, data: {turbolinks: false} %>
  <table class="table">
    <thead>
    <tr>
      <th scope="col">date</th>
      <th scope="col">kind</th>
      <th scope="col"></th>
    </tr>
    </thead>
    <tbody>
    <% @workouts.each do |w| %>
      <tr>
        <td><%= w.date %></td>
        <td><%= w.kind %></td>
        <td>
          <%= link_to '編集', edit_workout_path(w), data: {turbolinks: false} %>
          <%= link_to '削除', w, method: :delete, data: {confirm: 'Are you sure?'} %>
        </td>
      </tr>
    <% end %>
    </tbody>
  </table>
</ul>
